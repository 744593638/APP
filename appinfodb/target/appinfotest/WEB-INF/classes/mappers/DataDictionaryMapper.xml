<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kgc.dao.DataDictionaryMapper">
    <select id="findtypename" resultType="DataDictionary">
 		select * from appinfodb.data_dictionary where typename="APP状态"
 	</select>

    <select id="findptname" resultType="DataDictionary">
 		select * from appinfodb.data_dictionary where typename="所属平台"
 	</select>
	<!--通过valueid查询valueName-->
	<!--<select id="findd" resultType="DataDictionary">
		select valueName from data_dictionary where typename="所属平台" and valueId=#{valueid}
	</select>-->
</mapper>